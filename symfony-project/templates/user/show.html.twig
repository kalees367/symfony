{% extends 'base.html.twig' %}

{% block title %}User Management{% endblock %}



{% block body %}

   
    <h1>{{'Users details'|trans }}</h1>

    <table class="table table-striped table-middle-aligned">
        <thead>
            
        </thead>
        <tbody>
      
        
            <tr>
                <td>First Name</td>
                <td>{{ user.firstName}}</td>
                
            </tr>
			<tr>
                <td>Last Name</td>
                <td>{{ user.lastName}}</td>
                
            </tr>
			<tr>
                <td>Email Id</td>
                <td>
                {{ user.email }}
                </td>
                
            </tr>
			<tr>
                <td> Mobile </td>
                <td>
                {{ user.mobile }}
                
                </td>
                
            </tr>
			<tr>
                <td> Date Of Birth </td>
                <td>{{ user.dateOfBirth}}</td>
                
            </tr>
			<tr>
                <td> Education </td>
                <td>
                {{ user.education }}
              
               </td>
                
            </tr>
			<tr>
                <td> Blood Group </td>
                <td>{{ user.bloodGroup}}</td>
                
            </tr>
			<tr>
                <td> Gender </td>
                <td>{{ user.gender}}</td>
                
            </tr>
			
        </tbody>
    </table>
   
{% endblock %}

